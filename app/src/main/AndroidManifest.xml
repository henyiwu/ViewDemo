<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henyiwu.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Henyiwu">
        <activity
            android:name="com.henyiwu.customview.barrage.BarrageActivity"
            android:exported="false" />
        <activity
            android:name="com.henyiwu.customview.wave.WaveActivity"
            android:exported="true" />
        <activity android:name="com.henyiwu.customview.sticker.StickerActivity" />
        <activity android:name="com.henyiwu.customview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>